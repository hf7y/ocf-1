#program{
	position: absolute;
	left: 0px;
	top: 0px;
	line-height: 0.1vh;
	overflow: hidden;
	text-align: center;
	font-size: 3.5vh;
	height: 100%;
	width: 100%;
	max-height: 100%;
	max-width: 100%;
}

#allegro {
	position: relative;
	bottom: 6.5vh;
	font-size: 11.5vw;
	padding-top: 9vh;	
}

#andante {
	position: relative;
	top: 10vh;
	right: 15vw;
	text-align: right;	
	font-size: 9vw;

}

#vivace{
	background: black;
	position: fixed; 
	bottom: 0px;
	height: 7vh;
	width: 100%; 
	font-size: 4vw;
	font-family: monospace;
	padding-top: 6vh;
}

a{
	color: white;
}

a:hover{
	color: black;
}

